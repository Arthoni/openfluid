prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/@INCLUDE_INSTALL_PATH@

Name: ofelib
Description: OpenFLUID engine library
Version: @ENGINE_FULL_VERSION@
Libs: -L${libdir} -lopenfluid-base -lopenfluid-buddies -lopenfluid-core
 -lopenfluid-engine -lopenfluid-io -lopenfluid-tools
Cflags: -I${includedir} -DOFELIB_VERSION=@ENGINE_FULL_VERSION@
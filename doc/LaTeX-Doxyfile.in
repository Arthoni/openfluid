DOXYFILE_ENCODING      = UTF-8

PROJECT_NAME           = @DOXYGEN_PROJECT_NAME@
PROJECT_NUMBER         = @OPENFLUID_VERSION_FULL@

OUTPUT_DIRECTORY       = @OFBUILD_DOCBUILD_DIR@

FULL_PATH_NAMES        = NO


INPUT                  = @OFBUILD_DOC_CONTENTS_DIR@
INPUT_ENCODING         = UTF-8
FILE_PATTERNS          = *.md
RECURSIVE              = YES
EXCLUDE                = .svn .git
EXCLUDE_SYMLINKS       = NO
IMAGE_PATH             = @OFBUILD_DOC_CONTENTS_DIR@/images

GENERATE_MAN     	     = NO
GENERATE_LATEX    	   = YES
GENERATE_HTML     	   = NO
GENERATE_HTMLHELP      = NO
GENERATE_RTF           = NO
GENERATE_XML           = NO
PDF_HYPERLINKS         = YES
USE_PDFLATEX           = YES
STRIP_CODE_COMMENTS    = NO

LATEX_OUTPUT           = latex
LATEX_SOURCE_CODE	     = NO

QUIET                  = YES
WARNINGS               = YES
WARN_IF_DOC_ERROR      = YES

ENABLED_SECTIONS       = DocIsLaTeX


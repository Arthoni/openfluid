DOXYFILE_ENCODING      = UTF-8

PROJECT_NAME           = @DOXYGEN_PROJECT_NAME@
PROJECT_NUMBER         = @OPENFLUID_VERSION_FULL@
PROJECT_LOGO           = @OFBUILD_DOC_CONTENTS_DIR@/openfluid.png

OUTPUT_DIRECTORY       = @OFBUILD_DIST_MAIN_DOC_DIR@

FULL_PATH_NAMES        = YES
STRIP_FROM_PATH        = @OFBUILD_DOC_SRCTOPARSE_DIR@
STRIP_FROM_INC_PATH    = @OFBUILD_DOC_SRCTOPARSE_DIR@


EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
EXTRACT_LOCAL_METHODS  = YES
EXTRACT_ANON_NSPACES   = NO
HIDE_UNDOC_MEMBERS     = NO
HIDE_UNDOC_CLASSES     = NO


INPUT                  = @OFBUILD_DOC_SRCTOPARSE_DIR@ \
                         @OFBUILD_DOC_CONTENTS_DIR@
INPUT_ENCODING         = UTF-8
FILE_PATTERNS          = *.h \
                         *.hh \
                         *.hxx \
                         *.hpp \
                         *.h++ \
                         *.dox
RECURSIVE              = YES
EXCLUDE                = .svn
EXCLUDE_SYMLINKS       = NO
IMAGE_PATH             = @OFBUILD_DOC_CONTENTS_DIR@
EXAMPLE_PATH           = @OFBUILD_DOC_CONTENTS_DIR@

GENERATE_MAN     	     = NO
GENERATE_LATEX    	   = NO
GENERATE_HTML     	   = YES
GENERATE_HTMLHELP      = NO
GENERATE_RTF           = NO
GENERATE_XML           = NO
SEARCHENGINE           = NO
PDF_HYPERLINKS         = NO
USE_PDFLATEX           = NO
STRIP_CODE_COMMENTS    = NO

HTML_FILE_EXTENSION    = .html
HTML_HEADER            = @OFBUILD_DOC_LAYOUT_DIR@/header.html
HTML_FOOTER            = @OFBUILD_DOC_LAYOUT_DIR@/footer.html
HTML_EXTRA_STYLESHEET  = @OFBUILD_DOC_LAYOUT_DIR@/custom.css
HTML_OUTPUT            = html

SEARCHENGINE		       = YES
SERVER_BASED_SEARCH    = NO
SHORT_NAMES			       = YES

QUIET                  = YES
WARNINGS               = YES
WARN_IF_DOC_ERROR      = YES

MAX_INITIALIZER_LINES  = 0

INCLUDED_BY_GRAPH      = NO

ENABLED_SECTIONS       = DocIsHTML


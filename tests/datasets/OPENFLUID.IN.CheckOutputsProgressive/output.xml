<?xml version="1.0" standalone="yes"?>
<openfluid>
  <output>

    <!-- dtformat can be predefined (6cols,iso) or using the strftime() format, default is iso -->
    <!-- colsep default is \t -->
    <files colsep=" " dtformat="%Y %m %d %H %M %S" commentchar="%">     
      <set name="fullA" unitsclass="UnitsA" unitsIDs="*" vars="*" />      
      <set name="fullB" unitsclass="UnitsB" unitsIDs="*" vars="*" />      
    </files>  
    
  </output>
</openfluid>

<?xml version="1.0" standalone="yes"?>
<openfluid>
  <output>

    <!-- dtformat can be predefined (6cols,iso) or using the strftime() format, default is iso -->
    <!-- colsep default is \t -->
    <files colsep=" " dtformat="%Y %m %d %H %M %S" commentchar="%">     
      <set name="full-default" unitsclass="TestUnits" unitsIDs="*" vars="*" />      
      <set name="partial-default" unitsclass="TestUnits" unitsIDs="1;5" vars="*" />
    </files>  

    <files colsep=" " dtformat="%Y%m%dT%H%M%S" commentchar="%" header="none">     
      <set name="full-none" unitsclass="TestUnits" unitsIDs="*" vars="*" />      
      <set name="partial-none" unitsclass="TestUnits" unitsIDs="1;5" vars="*" />
    </files>  

    <files colsep=" " dtformat="%Y %m %d %H %M %S" commentchar="%" header="info">     
      <set name="full-info" unitsclass="TestUnits" unitsIDs="*" vars="*" />      
      <set name="partial-info" unitsclass="TestUnits" unitsIDs="1;5" vars="*" />
    </files>  

    <files colsep=";" dtformat="%Y-%m-%d %H:%M:%S" commentchar="%" header="colnames-as-data">     
      <set name="full-colnames-as-data" unitsclass="TestUnits" unitsIDs="*" vars="*" />      
      <set name="partial-colnames-as-data" unitsclass="TestUnits" unitsIDs="1;5" vars="*" />
    </files>  

    <files colsep=";" dtformat="%Y %m %d %H %M %S" commentchar="%" header="full">     
      <set name="full-full" unitsclass="TestUnits" unitsIDs="*" vars="*" />      
      <set name="partial-full" unitsclass="TestUnits" unitsIDs="1;5" vars="*" />
    </files>  

    
  </output>
</openfluid>

# Makefile options generated by configure script.


# platform
PLATFORM = i486-pc-linux-gnu

# C compilator
CC = gcc

# C/C++ compilator
CPP = g++

# Fortran77 compilator
F77 = 

# compile options for GEOS library
GEOSLIBS = -L/usr/lib -lgeos
GEOSFLAGS = -I/usr/include

# compile options for wxWidgets library
WXLIBS = -pthread   -lwx_baseu-2.6 
WXFLAGS = -I/usr/lib/wx/include/gtk2-unicode-release-2.6 -I/usr/include/wx-2.6 -DGTK_NO_CHECK_CASTS -D__WXGTK__ -D_FILE_OFFSET_BITS=64 -D_LARGE_FILES -D_LARGEFILE_SOURCE=1 -DNO_GCC_PRAGMA  

# common compile options
CCCLIBS =
# common compile flags
CCCFLAGS = -g -finput-charset=iso-8859-1

# main executable file
EXEFILE = mhydas-mpp

# plugins extension
PLUGINEXT = so

# dynamic library  extension
DYNLIBEXT = so

# build base dir
BASEBUILDDIR = /home/fabrejc/Labo/MHYDAS/MHYDAS-MPP/Dev/trunk/src/../../build/i486-pc-linux-gnu
# binary dir
BASEBINDIR = $(BASEBUILDDIR)/bin
# built objects dir
BASEOBJDIR = $(BASEBUILDDIR)/obj
# built libs dir
BASELIBDIR = $(BASEBUILDDIR)/lib
# built doc dir
BASEDOCDIR = /home/fabrejc/Labo/MHYDAS/MHYDAS-MPP/Dev/trunk/src/../../doc

# built plugins subdir
PLUGSUBDIR = plugs

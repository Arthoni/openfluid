
SET(CPACK_CMAKE_GENERATOR "@CMAKE_GENERATOR@")
SET(CPACK_GENERATOR "ZIP")


SET(CPACK_INSTALL_CMAKE_PROJECTS "@PROJECT_BINARY_DIR@;@PROJECT_NAME@;engine;/") 
SET(CPACK_PACKAGE_NAME "@ENGINE_PACK_NAME@")
SET(CPACK_PACKAGE_VERSION "@ENGINE_FULL_VERSION@")
SET(CPACK_PACKAGE_FILE_NAME "@ENGINE_PACK_NAME@_@ENGINE_FULL_VERSION@_win32")
SET(CPACK_PACKAGE_DESCRIPTION "@ENGINE_PACK_DESC@ This package has been built using the boost libraries v@Boost_MAJOR_VERSION@.@Boost_MINOR_VERSION@.")

SET(CPACK_PACKAGE_CONTACT "@PACKS_MAINTAINER@")
SET(CPACK_PACKAGE_DESCRIPTION_SUMMARY "@ENGINE_PACK_DESC@ This package has been built using the boost libraries v@Boost_MAJOR_VERSION@.@Boost_MINOR_VERSION@.")
